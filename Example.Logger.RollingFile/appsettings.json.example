{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Information"
      }
    },
    "Console": {
      "LogLevel": {
        "Microsoft.AspNetCore.Mvc.Razor.Internal": "Warning",
        "Microsoft.AspNetCore.Mvc.Razor.Razor": "Debug",
        "Microsoft.AspNetCore.Mvc.Razor": "Error",
        "Default": "Information"
      }
    },
    "RollingFile": {
      "LogLevel": {
        "Default": "Information"
      },
      "Path": "logs/{Date}/test.log",
      "MaxFileSize": "2MB"
    },
    "LogLevel": {
      "Default": "Debug"
    }
  }
}
